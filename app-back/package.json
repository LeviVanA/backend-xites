{
  "name": "app-back",
  "version": "1.0.0",
  "description": "back-end xites app",
  "main": "index.js",
  "author": "Levi Van Achter",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.1",
    "argon2": "^0.31.2",
    "axios": "^1.6.3",
    "config": "^3.3.9",
    "joi": "^17.11.0",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.0.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^7.0.2",
    "koa-jwt": "^4.0.4",
    "mysql2": "^3.6.3",
    "node-emoji": "1.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "nodemon": "^3.0.1"
  },
  "scripts": {
    "start": "npx env-cmd nodemon src/index.js"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src",
      "index.js"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  }
}
